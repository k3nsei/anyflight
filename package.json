  "author": "The Anyflight open source project",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "build": "npx nx run-many --target=build --all",
    "build:icons": "npx nx build icons",
    "build:ui": "npx nx run ui:build",
    "build:ui-storybook": "npx nx run ui:build-storybook --docsMode",
    "build:utils": "npx nx build utils",
    "format:check": "npx prettier --check --ignore-unknown .",
    "format:write": "npx prettier --write --ignore-unknown .",
    "prepare": "npx is-ci || husky install",
    "serve:ui": "npx nx run ui:storybook",
    "update": "npx nx migrate latest"
  },
