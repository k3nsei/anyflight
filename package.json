{
  "name": "anyflight",
  "version": "0.0.0",
  "private": true,
  "description": "Config driven modern UI for Betaflight/EmuFlight/INAV",
  "homepage": "https://github.com/k3nsei/anyflight",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k3nsei/anyflight.git"
  },
